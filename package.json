{
  "name": "fssgis",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "workspaces": [
    "clis/*",
    "packages/*",
    "projects/*"
  ],
  "scripts": {
    "publish": "lerna publish --amend --conventional-commits --no-push",
    "dev:docs": "npx vitepress dev ./projects/fssgis-docs/docs --host=0.0.0.0",
    "dev:docs.next": "npx vitepress dev ./projects/docs",
    "build:docs.next": "npx vitepress build ./projects/docs",
    "dev:sszhpsythpt": "vite --config ./projects/sszhpsythpt/vite.config.js",
    "build:sszhpsythpt": "vite build --config ./projects/sszhpsythpt/vite.config.js",
    "dev:sszhpsythpt-psyzt": "vite --config ./projects/sszhpsythpt-psyzt/vite.config.js",
    "build:sszhpsythpt-psyzt": "vite build --config ./projects/sszhpsythpt-psyzt/vite.config.js",
    "build": "npx rollup --config",
    "build:all": "npx rollup --config ./build/index.js",
    "build:docs": "npx vitepress build ./projects/fssgis-docs/docs",
    "build:axios": "npx rollup --config ./build/build.fssgis-axios.js -w",
    "build:ext": "npx rollup --config ./build/build.fssgis-ext.js -w",
    "build:observable": "npx rollup --config ./build/build.fssgis-observable.js -w",
    "build:utils": "npx rollup --config ./build/build.fssgis-utils.js -w",
    "build-api:utils": "npx typedoc --hideGenerator --disableSources --out ./projects/docs/public/api-docs/fssgis-utils ./packages/fssgis-utils/src/index.ts",
    "build:watcher": "npx rollup --config ./build/build.fssgis-watcher.js -w",
    "build:leaflet": "npx rollup --config ./build/build.fssg-leaflet.js -w",
    "build:map": "npx rollup --config ./build/build.fssg-map.js -w",
    "build:esri": "npx rollup --config ./build/build.fssg-esri.js -w",
    "build-api:esri": "npx typedoc --hideGenerator --disableSources --out ./projects/fssgis-docs/docs/public/api-docs/fssg-esri ./packages/fssg-esri/src/index.ts",
    "build:esri-hooks": "npx rollup --config ./build/build.fssg-esri-hooks.js -w",
    "build:grid": "npx rollup --config ./build/build.fssgis-grid.js -w",
    "build:ui": "npx rollup --config ./build/build.fssgis-ui.js -w",
    "test": "npx jest",
    "test:dev": "npx jest --watch",
    "test:vite": "vite --config ./packages/test-vite/vite.config.js",
    "dev:map-configuration": "vite --config ./projects/map-configuration/vite.config.js",
    "build:map-configuration": "vite build --config ./projects/map-configuration/vite.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/arcgis-js-api": "^4.20.1",
    "@types/jest": "^26.0.24",
    "@types/leaflet": "^1.7.4",
    "@types/proj4leaflet": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "@vitejs/plugin-vue": "^1.4.0",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.2.3",
    "eslint": "^7.31.0",
    "eslint-plugin-vue": "^7.14.0",
    "nodemon": "^2.0.12",
    "rollup": "^2.54.0",
    "rollup-jest": "^1.1.2",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.36.0",
    "sass-loader": "^12.1.0",
    "ts-jest": "^27.0.4",
    "typedoc": "^0.21.6",
    "typescript": "^4.3.4",
    "vite": "^2.4.4",
    "vite-plugin-windicss": "^1.4.2",
    "vitepress": "^0.16.1",
    "windicss": "^3.1.7"
  },
  "dependencies": {
    "vue": "^3.2.3"
  }
}
